<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">Appeul Snippets</div>
                <div class="o_panel_body">
                    <t t-snippet="theme_web_ap_travelagent.appaeul_explore_hotels" t-thumbnail="/theme_yourhome/static/src/img/snippets/agents.svg">
                        <keywords>hotels</keywords>
                    </t>

                </div>
            </div>
        </xpath>
    </template>


</odoo>